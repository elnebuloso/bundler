{
    "name": "elnebuloso\/bundler",
    "type": "library",
    "license": "MIT",
    "description": "bundle your stylesheets and your javascripts",
    "keywords": [
        "bundling",
        "minifying",
        "javascript",
        "js",
        "stylesheet",
        "css"
    ],
    "authors": [
        {
            "name": "Jeff Tunessen",
            "email": "jeff.tunessen@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony\/console": "~2.5",
        "symfony\/filesystem": "~2.5",
        "zendframework\/zend-log": "~2.3"
    },
    "suggest": {
        "elnebuloso\/bundler-markup": "markup renderer for bundler"
    },
    "autoload": {
        "psr-0": {
            "Bundler\\": "src\/"
        }
    },
    "bin": [
        "bin\/bundler"
    ]
}